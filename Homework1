import Java.util.ArrayList;
class Solution {
      public List<Integer> sortArrayByParity(int[] nums) {
         List<Integer> rightSide = new ArrayList<>();
         List<Integer> leftSide = new ArrayList<>();
        for(int num : nums){
            if(num %2 = 0){
                rightSide.add(num);
            }
            else{
                leftSide.add(num);
            }
        rightSide.AddAll(leftSide);

        }
        return rightSide;
    }
}
